HTTP: (method=METHOD)? http=URL  (lines=LINES)? (payload=PAYLOAD)? (output=TOFILE)?;
LINE: (query=QUERY) | (header=HEADER);
LINES: lines *= LINE;
URL: 'url' '(' url=STRING ')';
HEADER: 'header' '(' key=STRING ',' value=STRING ')';
METHOD: 'method' '(' type=METHODTYPE ')';
METHODTYPE: "GET" | "POST" | "OPTIONS" | "DELETE" | "CONNECT" | "PUT" | "HEAD" | "TRACE" ;
QUERY: 'query' '(' key=STRING ',' value=STRING ')';
PAYLOAD: 'data' '(' data=STRING ')' | 'fileinput' '(' file=STRING ')';
TOFILE: 'output' '('output=STRING ')';